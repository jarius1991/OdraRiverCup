#: kappa/Lib/site-packages/django/contrib/postgres/apps.py:12
msgid "PostgreSQL extensions"
msgstr "Extensions de PostgreSQL"

#: kappa/Lib/site-packages/django/contrib/postgres/fields/array.py:22
#: kappa/Lib/site-packages/django/contrib/postgres/forms/array.py:15
#: kappa/Lib/site-packages/django/contrib/postgres/forms/array.py:145
#, python-format
msgid "Item %(nth)s in the array did not validate: "
msgstr "L'element %(nth)s de la matriu no valida:"

#: kappa/Lib/site-packages/django/contrib/postgres/fields/array.py:23
msgid "Nested arrays must have the same length."
msgstr "Les matrius niades han de tenir la mateixa longitud."

